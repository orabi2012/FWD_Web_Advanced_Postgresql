{
    "name": "storefront_backend",
    "version": "0.1.0",
    "description": "",
    "main": "server.ts",
    "scripts": {
        "start": "nodemon ./src/server.ts",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
        "down": "db-migrate --env test down",
        "up:dev": "db-migrate --env dev up -c 10",
        "down:dev": "db-migrate --env dev down -c 10",
        "test": "set NODE_ENV=test && db-migrate --env test up && npx tsc && jasmine && db-migrate --env test reset",
        "build": "npx tsc",
        "jasmine": "set NODE_ENV=test && npx tsc && jasmine ",
        "tsc": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "format": "prettier --write src/**/*.ts"
    },
    "author": "Udacity",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.17.1",
        "pg": "^8.5.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.9",
        "@types/jasmine": "^3.6.3",
        "@types/morgan": "^1.9.3",
        "@types/pg": "^7.14.7",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^3.6.4",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts": "^0.3.0",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.7.3"
    }
}
